{
    "name": "stuarthaas/php-test",
    "minimum-stability": "dev",
    "scripts": {
        "test": [
            "./vendor/bin/phpunit --testdox tests"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
          "php": "7.0"
        }
    },
    "autoload": {
      "psr-4": {
          "app\\": "src/"
      }
  },
    "require": {
        "twig/twig": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
